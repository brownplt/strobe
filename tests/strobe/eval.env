type Eval = 
     rec evalFun . forall evalObj <: {{ eval : ^('evalFun) }} . [ref 'evalObj] -> Num

type Evalable =
     forall obj <: {{ eval : ^(Eval) }} . ref 'obj

type EvalPlus =
     rec plusEvalFun . 
        forall evalable <: {{ eval : ^(Eval) }} .
        forall plusObj <: {{ eval : ^('plusEvalFun), left : ref 'evalable, right : ref 'evalable }} . [ref 'plusObj] -> Num

type EvalNum =
     rec numEvalFun .
        forall numObj <: {{ eval : ^('numEvalFun), "val" : Num }} . [ref 'numObj] -> Num


     
                                                        
                                                       
     
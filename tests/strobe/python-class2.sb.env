type Point_dist =
     forall o <: {{x: Num, y : Num}} . [ref 'o] -> Num

type Point =
     { dist : 'Point_dist, "__proto__" : Null, *: _ }

type ptPr =
     { dist : [] -> Num, "__proto__" : Null, *: _}

type ColorPoint_setColor =
     forall o <: {{x: Num, y : Num, color : Str}} . [ref 'o] Str -> Undef

type cpt = {{x: Num, y : Num, color : Str, dist :_, setColor: _,
	     "__class__" : {setColor : 'ColorPoint_setColor, "__proto__" : 'Point, * : _},
	     "__proto__" : {setColor : [Str] -> Undef, "__proto__" : 'ptPr, * : _} }}


     
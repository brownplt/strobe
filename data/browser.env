// HTMLWindow is the type of the global object. All its fields are added to the
// initial environment.
class HTMLWindow prototype Global {
  document : HTMLDocument,
  setTimeout : (-> Undef) * Int -> Int,
  setInterval : ( -> Undef) * Int -> Int,
  clearInterval : Int -> Undef,
  clearTimeout : Int -> Undef,
  window : HTMLWindow,
}

class HTMLElement  {
}

class HTMLDocument {
  getElementById : String -> Null + HTMLElement,
  onclick : Event -> Undef,
  onmousemove : Event -> Undef
}

class HTMLLabel {
  innerHTML : String
}

class Event {
  pageX : Int,
  pageY : Int
}

//canvas!
class Canvas {
  getContext : String -> CanvasRenderingContext2D
}

class CanvasGradient {
  addColorStop : Double * String -> Undef
}

class CanvasPattern {
}

class CanvasRenderingContext2D {
  clearRect : Double * Double * Double * Double -> Undef,
  fillStyle : String + CanvasGradient + CanvasPattern,
  fillRect : Double * Double * Double * Double -> Undef,
  beginPath : -> Undef,
  closePath : -> Undef,
  arc : Double * Double * Double * Double * Double * Boolean -> Undef,
  fill : -> Undef
}


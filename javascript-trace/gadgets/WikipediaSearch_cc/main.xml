  <div name="bardiv" x="0" y="0" width="410" height="110" visible="false">
    <img name="bar" src="images/bar_default.png" x="0" y="0" />
    <img name="field" src="images/field_focus.png" x="179" y="68" />
    <button name="searchdrop" image="images/searchdrop_default.png"
      x="182" y="68" overImage="images/searchdrop_hover.png"
      downImage="images/searchdrop_down.png" onClick="onSearchDropClick();" enabled="true" />
    <edit name="textbox" x="204" y="71" width="155" height="14" size="9"
      onFocusIn="onTextBoxFocusIn();" onFocusOut="onTextBoxFocusOut();"
      onChange="onTextBoxChange();" onKeyPress="onTextBoxKeyPress();"
      onClick="onTextBoxClick();" />
    <div name="throbber" x="361" y="70" width="20" height="20" />
  </div>

  <div name="resultsdivparent" x="28" y="90" width="368" height="341" visible="false">
    <div name="resultsdiv" x="0" y="0" width="368" height="341" opacity="230" visible="false">
      <img name="results" src="images/results_default.png" x="0" y="0" />
      <label name="articletext" x="6" y="11" width="258" height="309"
        trimming="word-ellipsis" wordWrap="true" font="Arial" size="10" />
      <img name="articleimage" src="" width="82" height="150" />
      <button name="popout" image="images/popout_default.png"
        overImage="images/popout_hover.png" downImage="images/popout_down.png"
        x="334" y="304" />
    </div>
  </div>

  <div name="bardiv_sidebar" x="0" y="0" width="170" height="60" visible="false">
    <img name="bar_sidebar" src="images/bar_default_sidebar.png" x="5" y="2" />
    <img name="field_sidebar" src="images/field_focus_sidebar.png" x="4" y="33" />
    <button name="searchdrop_sidebar" image="images/searchdrop_default.png"
      x="6" y="34" overImage="images/searchdrop_hover.png"
      downImage="images/searchdrop_down.png" onClick="onSearchDropClick();" />
    <edit name="textbox_sidebar" x="28" y="36" width="117" height="15" size="9"
      onFocusIn="onTextBoxFocusIn();" onFocusOut="onTextBoxFocusOut();"
      onChange="onTextBoxChange();" onKeyPress="onTextBoxKeyPress();" />
    <div name="throbber_sidebar" x="147" y="35" width="20" height="20" />
  </div>

  <div name="historydiv" x="203" y="85" width="175" height="85" />
</view>
